                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass ethon:ContractAccount
                                  ] ;
                  ethon:suggestedStringRepresentation "ContractMsg" ;
                  rdfs:comment "A contract message is passed between a contract account and any other account (external or contract). It is the result of an execution chain originally triggered by an external eccount."@en ;
                  rdfs:label "Contract Message"@en ;
                  ns:term_status "unstable" .


