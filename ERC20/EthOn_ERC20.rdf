        <dc:creator>Johannes Pfeffer</dc:creator>
        <ns:term_status>testing</ns:term_status>
        <rdfs:seeAlso rdf:resource="https://github.com/ethereum/wiki/wiki/White-Paper"/>
        <dc:contributor>Joseph Chow</dc:contributor>
        <rdfs:comment xml:lang="en">The EthOn Ethereum ontology, described using W3C RDF Schema and the Web Ontology Language. It is closely aligned with Gavin Wood&apos;s Ethereum yellow paper.</rdfs:comment>
        <dc:contributor>Casey Detrio</dc:contributor>
        <dc:contributor>Maurycy Pietrzak</dc:contributor>
        <dc:contributor>Alex Beregszazi</dc:contributor>
        <rdfs:seeAlso rdf:resource="https://github.com/ConsenSys/EthOn"/>
        <dc:title>EthOn - An Ethereum Ontology</dc:title>
        <dc:contributor>Herman Junge</dc:contributor>
        <owl:versionInfo>0.2</owl:versionInfo>
